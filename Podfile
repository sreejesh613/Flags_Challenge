source 'https://github.com/CocoaPods/Specs.git'
#platform :ios, '10.0'

target 'Flags Challenge' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Flags Challenge
  pod 'AEOTPTextField'

  def testing_pods
      pod 'Quick'
      pod 'Nimble'
  end

  target 'Flags ChallengeTests' do
    inherit! :search_paths
    # Pods for testing
    testing_pods
  end

  target 'Flags ChallengeUITests' do
    # Pods for testing
    testing_pods
  end

end
